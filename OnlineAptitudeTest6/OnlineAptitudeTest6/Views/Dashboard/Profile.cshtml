@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Edit</h1>

@model OnlineAptitudeTest6.Models.RegistrationRoleViewModel

<form asp-action="Profile" method="POST" enctype="multipart/form-data">

    @*    <input type="hidden" asp-for="productTable.Id" /> *@

    <div class="input-group input-group-outline mb-3">
        <input asp-for="Registration.Username" placeholder="User Name" class="form-control" />
    </div>

    <div class="input-group input-group-outline mb-3">
        <input asp-for="Registration.Password" placeholder="Password" class="form-control" />
    </div>

     

    <div class="input-group input-group-outline mb-3">
        <input asp-for="Registration.PersonalDetails" placeholder="PersonalDetails" class="form-control" />
    </div>

    <div class="input-group input-group-outline mb-3">
        <input asp-for="Registration.EducationDetails" placeholder="EducationDetails" class="form-control" />
    </div>

    <div class="input-group input-group-outline mb-3">
        <input asp-for="Registration.WorkExperience" placeholder="WorkExperience" class="form-control" />
    </div>

    <div class="input-group input-group-outline mb-3">
        <input type="file" id="file" name="newimg" class="form-control" />
    </div>
    <div class="btn btn-lg bg-gradient-primary btn-lg w-100 mt-4 mb-0">
        <input type="submit" value="Update"></input>

    </div>
</form>