
@{
    ViewData["Title"] = "Edittest";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Edittest</h1>

    @model OnlineAptitudeTest6.Models.Test







<div class="container-fluid py-4">
    <div class="row">
        <div class="col-12">
            <div class="card my-4">
                <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2">
                    <div class="bg-gradient-primary shadow-primary border-radius-lg pt-4 pb-3">
                        <h6 class="text-white text-capitalize ps-3">Edit Role</h6>
                    </div>
                </div>
                <div class="container">
                    <form asp-action="Edittest" method="post">


                        <input type="hidden" class="form-control" asp-for="TestId" />
                        <div class="form-group mt-2">
                           
                            <div class="input-group input-group-outline mb-3">

                                <input type="text" class="form-control " asp-for="TestName" />
                            </div>
                            <div class="input-group input-group-outline mb-3">

                                <input type="text" class="form-control" asp-for="TotalQuestions" />
                            </div>
                            <div class="input-group input-group-outline mb-3">
                                <input type="text" class="form-control" asp-for="TotalMarks" />
                            </div>
                            <div class="input-group input-group-outline mb-3">
                                <input type="text" class="form-control" asp-for="TimeLimit" />
                            </div>
                        </div>

                        <button type="submit" class="btn btn-primary">Update</button>
                    </form>
                </div>
